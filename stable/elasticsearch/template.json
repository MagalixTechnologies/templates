{
	"description": "",
	"name": "elasticsearch",
	"services": [{
		"containers": [{
			"description": "intended for clustering management only, no data, no HTTP API",
			"environment": [{
				"name": "CLUSTER_NAME",
				"value": "docker-cluster"
			}, {
				"name": "NODE_MASTER",
				"value": "true"
			}, {
				"name": "NODE_DATA",
				"value": "false"
			}, {
				"name": "BOOTSTRAP_MEMORY_LOCK",
				"value": "true"
			}, {
				"name": "ES_JAVA_OPTS",
				"value": "-Xmx2g -Xms2g"
			}, {
				"name": "HTTP_ENABLE",
				"value": "false"
			}],
			"image_url": "docker.elastic.co/elasticsearch/elasticsearch:6.2.3",
			"name": "es-master",
			"ports": [{
				"container_port": 9300,
				"name": "tcp"
			}],
			"resources": {
				"io": 100,
				"memory": 4096,
				"vcpu": 2
			},
			"volume_mounts": [{
				"mount_path": "/usr/share/elasticsearch/data",
				"name": "es-master"
			}]
		}],
		"description": "",
		"name": "elasticsearch",
		"ports": [{
			"is_public": false,
			"is_websocket": false,
			"port": 9300,
			"target_port": "tcp"
		}],
		"resources": {
			"replicas": 1
		},
		"restart": "always"
	}, {
		"containers": [{
			"environment": [{
				"name": "ELASTICSEARCH_URL",
				"value": "http://es-client:9200"
			}],
			"image_url": "docker.elastic.co/kibana/kibana:6.2.3",
			"name": "kibana",
			"ports": [{
				"container_port": 5601,
				"name": "web"
			}],
			"resources": {
				"io": 50,
				"memory": 4096,
				"vcpu": 2
			}
		}],
		"description": "",
		"name": "kibana",
		"ports": [{
			"is_public": true,
			"is_websocket": false,
			"path": "/",
			"port": 5601,
			"target_port": "web"
		}],
		"resources": {
			"replicas": 1
		},
		"restart": "always"
	}, {
		"containers": [{
			"commands": ["/bin/bash", "-c", "echo input {twitter {consumer_key =\\> \\\"\\${TWITTER_CONSUMER_KEY}\\\" consumer_secret =\\> \\\"\\${TWITTER_CONSUMER_SECRET}\\\" oauth_token =\\> \\\"\\${TWITTER_OAUTH_TOKEN}\\\" oauth_token_secret =\\> \\\"\\${TWITTER_OAUTH_TOKEN_SECRET}\\\" keywords =\\> [\\\"\\${TWITTER_KEYWORDS}\\\"] full_tweet =\\> true}} output {elasticsearch {hosts =\\> \\\"es-client:9200\\\"}} >  /usr/share/logstash/pipeline/logstash.conf; bin/logstash -f /usr/share/logstash/pipeline/logstash.conf --config.reload.automatic"],
			"environment": [{
				"name": "TWITTER_CONSUMER_KEY",
				"value": "CONSUMER_KEY_GOES_HERE"
			}, {
				"name": "TWITTER_CONSUMER_SECRET",
				"value": "CONSUMER_SECRET_GOES_HERE"
			}, {
				"name": "TWITTER_OAUTH_TOKEN",
				"value": "ACCESS_TOKEN_GOES_HERE"
			}, {
				"name": "TWITTER_OAUTH_TOKEN_SECRET",
				"value": "ACCESS_TOKEN_SECRET_GOES_HERE"
			}, {
				"name": "TWITTER_KEYWORDS",
				"value": "Seach,keywords,separated,by,commas"
			}],
			"image_url": "docker.elastic.co/logstash/logstash:6.2.3",
			"name": "logstash",
			"ports": [{
				"container_port": 9600,
				"name": "http"
			}],
			"resources": {
				"io": 10,
				"memory": 2048,
				"vcpu": 1
			}
		}],
		"description": "",
		"name": "logstash",
		"ports": [{
			"is_public": true,
			"is_websocket": false,
			"path": "/",
			"port": 9600,
			"target_port": "http"
		}],
		"resources": {
			"replicas": 1
		},
		"restart": "always"
	}, {
		"containers": [{
			"description": "intended for client usage, no data, with HTTP API",
			"environment": [{
				"name": "DISCOVERY_ZEN_PING_UNICAST_HOSTS",
				"value": "elasticsearch"
			}, {
				"name": "NODE_MASTER",
				"value": "false"
			}, {
				"name": "NODE_DATA",
				"value": "false"
			}, {
				"name": "BOOTSTRAP_MEMORY_LOCK",
				"value": "true"
			}, {
				"name": "CLUSTER_NAME",
				"value": "docker-cluster"
			}, {
				"name": "HTTP_ENABLE",
				"value": "true"
			}],
			"image_url": "docker.elastic.co/elasticsearch/elasticsearch:6.2.3",
			"name": "es-client",
			"ports": [{
				"container_port": 9200,
				"name": "http"
			}, {
				"container_port": 9300,
				"name": "tcp"
			}],
			"resources": {
				"io": 100,
				"memory": 4096,
				"vcpu": 2
			},
			"volume_mounts": [{
				"mount_path": "/usr/share/elasticsearch/data",
				"name": "es-client"
			}]
		}],
		"description": "",
		"name": "es-client",
		"ports": [{
			"is_public": true,
			"is_websocket": false,
			"path": "/",
			"port": 9200,
			"target_port": "http"
		}, {
			"is_public": false,
			"is_websocket": false,
			"port": 9300,
			"target_port": "tcp"
		}],
		"resources": {
			"replicas": 1
		},
		"restart": "always"
	}, {
		"containers": [{
			"description": "intended for storing and indexing data, no HTTP API",
			"environment": [{
				"name": "DISCOVERY_ZEN_PING_UNICAST_HOSTS",
				"value": "elasticsearch"
			}, {
				"name": "NODE_MASTER",
				"value": "false"
			}, {
				"name": "NODE_DATA",
				"value": "true"
			}, {
				"name": "BOOTSTRAP_MEMORY_LOCK",
				"value": "true"
			}, {
				"name": "CLUSTER_NAME",
				"value": "docker-cluster"
			}, {
				"name": "HTTP_ENABLE",
				"value": "false"
			}],
			"image_url": "docker.elastic.co/elasticsearch/elasticsearch:6.2.3",
			"name": "es-data",
			"ports": [{
				"container_port": 9300,
				"name": "transport"
			}],
			"resources": {
				"io": 100,
				"memory": 4096,
				"vcpu": 2
			},
			"volume_mounts": [{
				"mount_path": "/usr/share/elasticsearch/data",
				"name": "es-data"
			}]
		}],
		"description": "",
		"name": "es-data",
		"ports": [{
			"is_public": false,
			"is_websocket": false,
			"port": 9300,
			"target_port": "transport"
		}],
		"resources": {
			"replicas": 1
		},
		"restart": "always"
	}],
	"target_cloud": {
		"region": "us-east-1",
		"type": "aws",
		"zone": "us-east-1a"
	},
	"volumes": [{
		"access_mode": "ReadWriteOnce",
		"name": "es-master",
		"persistent": true,
		"size": 4
	}, {
		"access_mode": "ReadWriteOnce",
		"name": "es-data",
		"persistent": true,
		"size": 10
	}, {
		"access_mode": "ReadWriteOnce",
		"name": "es-client",
		"persistent": true,
		"size": 4
	}]
}
