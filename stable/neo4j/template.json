{
	"description": "Neo4j The graph database platform powering todays mission-critical enterprise applications, including artificial intelligence, fraud detection and recommendations.",
	"name": "neo4j",
	"services": [{
		"containers": [{
			"environment": [{
				"name": "NEO4J_AUTH",
				"value": "none"
			}],
			"image_url": "btibert3/neo4j-apoc-jdbc",
			"name": "neo4j",
			"ports": [{
				"container_port": 7474,
				"name": "http"
			}, {
				"container_port": 7687,
				"name": "bolt"
			}],
			"resources": {
				"io": 10,
				"memory": 8192,
				"vcpu": 1
			},
			"volume_mounts": [{
				"mount_path": "/data",
				"name": "neo4j-data"
			}, {
				"mount_path": "/conf/",
				"name": "neo4j-conf"
			}, {
				"mount_path": "/var/lib/neo4j/import",
				"name": "neo4j-import"
			}]
		}],
		"description": "Neo4j The graph database platform powering todays mission-critical enterprise applications, including artificial intelligence, fraud detection and recommendations.",
		"name": "neo4j",
		"ports": [{
			"is_public": true,
			"is_websocket": true,
			"path": "/",
			"port": 80,
			"target_port": "http"
		}, {
			"is_public": true,
			"is_websocket": true,
			"path": "/",
			"port": 7687,
			"target_port": "bolt"
		}],
		"resources": {
			"replicas": 1
		},
		"restart": "always"
	}],
	"target_cloud": {
		"region": "us-east-1",
		"type": "aws",
		"zone": "us-east-1a"
	},
	"volumes": [{
		"access_mode": "ReadWriteOnce",
		"name": "neo4j-data",
		"persistent": true,
		"size": 4
	}, {
		"access_mode": "ReadWriteOnce",
		"name": "neo4j-conf",
		"persistent": true,
		"size": 4
	}, {
		"access_mode": "ReadWriteOnce",
		"name": "neo4j-import",
		"persistent": true,
		"size": 4
	}]
}