{
	"name": "spark-zeppelin",
	"description": "Fast and general-purpose cluster computing system.",
	"target_cloud": {
		"type": "aws",
		"region": "us-east-1",
		"zone": "us-east-1a"
	},
	"labels": [],
	"services": [{
		"name": "zeppelin",
		"labels": [],
		"containers": [{
			"name": "zeppelin",
			"description": "Apache Zeppelin interpreter concept allows any language data-processing-backend to be plugged into Zeppelin.",
			"image_url": "gcr.io/google_containers/zeppelin:v0.5.5_v2",
			"commands": ["/bin/sh", "-c", "sed -i.bak s/spark-master:7077/spark-master:7077/g /opt/zeppelin/conf/zeppelin-env.sh; /opt/zeppelin/bin/docker-zeppelin.sh"],
			"ports": [{
				"name": "p8080",
				"container_port": 8080
			}],
			"labels": [],
			"environment": [],
			"volume_mounts": []
		}],
		"resources": {
			"vcpu": 1,
			"memory": 512,
			"io": 10,
			"replicas": 1
		},
		"ports": [{
			"is_public": true,
			"target_port": "p8080",
			"port": 8080
		}]
	}, {
		"name": "spark-worker",
		"labels": [],
		"containers": [{
			"name": "spark-worker",
			"image_url": "gcr.io/google_containers/spark:1.5.1_v3",
			"commands": ["/opt/spark/bin/spark-class", "org.apache.spark.deploy.worker.Worker", "spark://spark-master:7077", "--webui-port", "8081"],
			"ports": [{
				"name": "p8081",
				"container_port": 8081
			}],
			"labels": [],
			"environment": [{
				"name": "SPARK_DAEMON_MEMORY",
				"value": "1g"
			}, {
				"name": "SPARK_WORKER_MEMORY",
				"value": "1g"
			}],
			"volume_mounts": []
		}],
		"resources": {
			"vcpu": 1,
			"memory": 512,
			"io": 10,
			"replicas": 3
		},
		"ports": [{
			"is_public": false,
			"target_port": "p8081",
			"port": 8081
		}]
	}, {
		"name": "spark-master",
		"labels": [],
		"containers": [{
			"name": "spark-master",
			"image_url": "gcr.io/google_containers/spark:1.5.1_v3",
			"commands": ["/bin/sh", "-c", "echo $(hostname -i) spark-master >> /etc/hosts; /opt/spark/bin/spark-class org.apache.spark.deploy.master.Master --ip spark-master --port 7077 --webui-port 8080"],
			"ports": [{
				"name": "p7077",
				"container_port": 7077
			}, {
				"name": "p8080",
				"container_port": 8080
			}],
			"labels": [],
			"environment": [{
				"name": "SPARK_DAEMON_MEMORY",
				"value": "1g"
			}],
			"volume_mounts": []
		}],
		"resources": {
			"vcpu": 1,
			"memory": 512,
			"io": 10,
			"replicas": 1
		},
		"ports": [{
			"is_public": true,
			"target_port": "p7077",
			"port": 7077
		}, {
			"is_public": true,
			"target_port": "p8080",
			"port": 8090
		}]
	}],
	"volumes": [],
	"kpi_policies": []
}